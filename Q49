#include <iostream>
#include <cmath>
using namespace std;

double power(double base, int exponent) {
    return pow(base, exponent);
}

double power(int base, double exponent) {
    return pow(base, exponent);
}

int main() {
    cout << "Power (int, int): " << power(2, 3) << endl;
    cout << "Power (int, double): " << power(4, 2.5) << endl;
    return 0;
}
