#include <iostream>
#include <cmath>
using namespace std;

// Version 1: Two integers
int power(int base, int exponent) {
    return pow(base, exponent);
}

// Version 2: One double and one integer
double power(double base, int exponent) {
    return pow(base, exponent);
}

int main() {
    cout << "power(int, int): " << power(2, 3) << endl;
    cout << "power(double, int): " << power(2.5, 3) << endl;
    return 0;
}
