#include <iostream>
using namespace std;

void calculateGrade(int marks) {
    if (marks >= 90) cout << "Grade: A\n";
    else if (marks >= 80) cout << "Grade: B\n";
    else if (marks >= 70) cout << "Grade: C\n";
    else if (marks >= 60) cout << "Grade: D\n";
    else cout << "Grade: F\n";
}

int main() {
    int marks;
    cout << "Enter marks: ";
    cin >> marks;
    
    calculateGrade(marks);
    return 0;
}
