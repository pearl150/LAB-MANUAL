#include <iostream>
using namespace std;

int area(int side) {
    return side * side;
}

int area(int length, int width) {
    return length * width;
}

double area(double radius) {
    return 3.1416 * radius * radius;
}

int main() {
    cout << "Area of square: " << area(5) << endl;
    cout << "Area of rectangle: " << area(5, 10) << endl;
    cout << "Area of circle: " << area(3.5) << endl;
    return 0;
}
