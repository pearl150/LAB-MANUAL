#include <iostream>
using namespace std;

string concat(string str1, string str2) {
    return str1 + str2;
}

string concat(int num, string str) {
    return to_string(num) + str;
}

int main() {
    cout << concat("Hello, ", "World!") << endl;
    cout << concat(10, " Apples") << endl;
    return 0;
}
