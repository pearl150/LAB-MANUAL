// Take two floating point numbers input and display their division result with two decimal places
#include <iostream>
#include <iomanip> 
using namespace std;
int main() {
    double num1, num2, result;
  cout << "Enter first number: ";
   cin >> num1;
cout << "Enter second number: ";
    cin >> num2;
 if (num2 == 0) {
 cout << "Error: Division by zero is not allowed." << std::endl;
  return 1; // Exit with error code
    }
 result = num1 / num2;
 cout << "Result: " << std::fixed << std::setprecision(2) << result << std::endl;

    return 0;
}
