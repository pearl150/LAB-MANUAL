// develop a program that takes two integers and determines if the first is a multiple of the second using a relational and logical operators
#include <iostream>

using namespace std;

int main() {
    int num1, num2;

    // Taking input
    cout << "Enter first number: ";
    cin >> num1;
    cout << "Enter second number: ";
    cin >> num2;

    // Checking if num1 is a multiple of num2
    if (num2 != 0 && num1 % num2 == 0)  
        cout << num1 << " is a multiple of " << num2 << endl;
    else
        cout << num1 << " is not a multiple of " << num2 << endl;

    return 0;
}
